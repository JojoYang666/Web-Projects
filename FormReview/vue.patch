+function updateStyle (oldVnode: VNodeWithData, vnode: VNodeWithData) {
+  if (!oldVnode.data.style && !vnode.data.style) {
+    return
+  }
+  let cur, name
+  const elm = vnode.elm
+  const oldStyle: any = oldVnode.data.style || {}
